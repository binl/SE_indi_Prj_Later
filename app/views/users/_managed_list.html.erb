<div id="managed_users_list">
<table class="table table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th style="width: 200px;">email</th>
    <th></th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% all_users.each do |user| %>
      <tr>
        <td><%= link_to(user.name, user) %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Delete', user, method: :delete, :remote => true %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</div>